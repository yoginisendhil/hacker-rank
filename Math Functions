#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    float a;
    int b, c, d;
    scanf("%f\n%d\n%d\n%d", &a, &b, &c, &d);
    printf("%.0f\n",floor(a));
    printf("%0.f\n", ceil(a));
    printf("%d\n", (int)sqrt(b));
    printf("%d", (int)pow(c,d));
    
    return 0;
}
