#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int a;
    float b;
    scanf("%d\n%f", &a, &b);
    printf("%d\n", a);
    printf("%.2f", b);
    return 0;
}
