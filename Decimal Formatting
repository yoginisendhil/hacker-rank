#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    float num;
    scanf("%f\n", &num);
    printf("%.5f\n", num);
    printf("%.4f\n", num);
    printf("%.3f\n", num);
    printf("%.2f\n", num);
    printf("%.1f\n", num);
    printf("%.0f\n", floor(num));
    return 0;
}
