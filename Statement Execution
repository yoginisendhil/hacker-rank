#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    int a;
    float b;
    char c;
    char d[100], e[100];
    scanf("%d %f %c", &a, &b, &c);
    scanf("%s ", d);
    fgets(e, 100, stdin);
    printf("Integer is %d\nFloat is %.2f\nCharacter is %c", a, b, c);
    printf("\nStrings are %s and %s", d, e);
    return 0;
}
